  <span class="nt">-H</span> <span class="s1">'Accept: */*'</span> <span class="se">\</span>
  <span class="nt">-H</span> <span class="s1">'Authorization: string'</span> <span class="se">\</span>
  <span class="nt">-H</span> <span class="s1">'x-synonym: string'</span>

</code></pre></div><div class="highlight"><pre class="highlight java tab-java"><code><span class="no">URL</span> <span class="n">obj</span> <span class="o">=</span> <span class="k">new</span> <span class="no">URL</span><span class="o">(</span><span class="s">"http://localhost:8098/request-management/api/v1/requests/comments/{requestId}"</span><span class="o">);</span>
